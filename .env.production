NODE_ENV=production

# public path
VITE_PUBLIC_PATH = /

VITE_APP_API_BASEURL = /api

# 删除log
VITE_DROP_CONSOLE = true

# 是否兼容低版本浏览器
VITE_LEGACY = false

# 是否开启图片压缩
VITE_USE_IMAGEMIN= true
# 生产时压缩算法
# Optional: gzip | brotli | none
VITE_BUILD_COMPRESS = 'gzip'

VITE_BUILD_COMPRESS_DELETE_ORIGIN_FILE = false
